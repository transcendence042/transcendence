{
  "ui": true,
  "disable_mlock": true,
  "listener": [
    { "tcp": { 
      "address": "0.0.0.0:8200",
      "tls_disable": 0,
      "tls_cert_file": "/vault/certs/fullchain.pem",
      "tls_key_file": "/vault/certs/privkey.pem"
    } }
  ],
  "storage": { "file": { "path": "/vault/file" } },
  "log_level": "info"
}
